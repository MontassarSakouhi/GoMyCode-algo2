ALGORITHM Fibonacci
VAR
    n, a, b, temp, i: INTEGER;

BEGIN
    write("Enter the value of n");
    read(n);
    
    a := 0;
    b := 1;
    
    FOR i FROM 0 TO n - 1 DO
        temp := a;
        a := b;
        b := temp + b;
    END FOR
    
    write("The Fibonacci number is ", a);
END




ALGORITHM Power
VAR
    n, p, result, i: INTEGER;

BEGIN
    write("Enter the base number (n)");
    read(n);
    
    write("Enter the power (p)");
    read(p);
    
    result := 1;
    
    FOR i FROM 0 TO p - 1 DO
        result := result * n;
    END FOR
    
    write("The result of n^p is ", result);
END


