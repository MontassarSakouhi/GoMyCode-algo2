ALGORITHM Ticket Pricing
VAR
    age: INTEGER;

BEGIN
    read(age);
    
    IF (age <= 12) THEN
        write("10$");
    ELSE IF (age >= 13 AND age <= 18) THEN
        write("15$");
    ELSE IF (age > 18) THEN
        write("20$");
    ELSE
        write("invalid age");
    END IF;
END



